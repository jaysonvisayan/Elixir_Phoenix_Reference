<style>
    body {
      font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif;
      color: #333;
    }

    .panel {
        margin-bottom: 10px;
        background-color: #fff;
        border: 1px solid transparent;
        border-radius: 4px;
        -webkit-box-shadow: 0 1px 1px rgba(0,0,0,.05);
        box-shadow: 0 1px 1px rgba(0,0,0,.05);
    }
    .panel-default {
        border-color: rgba(34, 36, 38, 0.15);
    }
    .panel-default>.panel-heading {
        color: #FFF;
        background-color: #388e3c;
        border-color: rgba(34, 36, 38, 0.15);
        text-align: center;
    }
    .panel-heading {
        padding: 7px 15px;
        border-bottom: 1px solid transparent;
        border-top-left-radius: 3px;
        border-top-right-radius: 3px;
        font-size: 12px;
    }

    .panel-body {
        padding: 15px;
    }

    table {
        text-align: left;
        border-collapse: separate;
        border-spacing: 0;
        width: 100%;
        font-size: 8px;
    }

    th:first-child,
    td:first-child {
        padding: 0 15px 0 15px;
    }


    tbody tr:last-child td {
        border: none;
    }

    tbody td {
        line-height: 20px;
    }

    .ui.small.images .image,
    .ui.small.images img,
    .ui.small.images svg,
    .ui.small.image {
        width: 150px;
        height: auto;
        font-size: 0.92857143rem;
    }

    .ui.medium.images .image,
    .ui.medium.images img,
    .ui.medium.images svg,
    .ui.medium.image {
        width: 300px;
        height: auto;
        font-size: 1rem;
    }

    .no-border {
        border: none !important;
    }

    .pl {
        padding-left: 25px !important;
    }

    .mt_zero {
        margin-top: 0 !important;
    }

    .header {
        border-bottom: 1px solid rgba(34, 36, 38, 0.1) !important;
    }

    .text-white {
        color: #fff !important;
    }

    h5{
        font-size: 9px !important;
        margin: 10px 15px !important;
    }

    .header {
        border: none;
        margin: calc(2rem - 0.14285714em ) 0em 1rem;
        padding: 0em 0em;
        font-weight: bold;
        line-height: 1.28571429em;
        text-transform: none;
        color: rgba(0, 0, 0, 0.87);
    }

    .dividing.header {
        padding-bottom: 0.21428571rem;
        border-bottom: 1px solid rgba(34, 36, 38, 0.15);
    }

    .ui.basic.table {
      /*background: transparent;*/
      border: 1px solid rgba(34, 36, 38, 0.15);
      box-shadow: none;
    }

    .ui.basic.table thead,
    .ui.basic.table tfoot {
      box-shadow: none;
    }

    .ui.basic.table th {
      /*background: transparent;*/
      border-left: none;
    }

    .ui.basic.table tbody tr {
      border-bottom: 1px solid rgba(0, 0, 0, 0.1);
    }

    .ui.basic.table td {
      background: transparent;
    }

    .ui.basic.striped.table tbody tr:nth-child(2n) {
      background-color: rgba(0, 0, 0, 0.05) !important;
    }

    .ui[class*="very basic"].table {
      border: none;
    }

    .ui[class*="very basic"].table:not(.sortable):not(.striped) th,
    .ui[class*="very basic"].table:not(.sortable):not(.striped) td {
      padding: '';
    }

    .ui[class*="very basic"].table:not(.sortable):not(.striped) th:first-child,
    .ui[class*="very basic"].table:not(.sortable):not(.striped) td:first-child {
      padding-left: 0em;
    }

    .ui[class*="very basic"].table:not(.sortable):not(.striped) th:last-child,
    .ui[class*="very basic"].table:not(.sortable):not(.striped) td:last-child {
      padding-right: 0em;
    }

    .ui[class*="very basic"].table:not(.sortable):not(.striped) thead tr:first-child th {
      padding-top: 0em;
    }

    .ui.table {
      width: 100%;
      /*background: #FFFFFF;*/
      margin: 1em 0em;
      /*border: 1px solid rgba(34, 36, 38, 0.15);*/
      box-shadow: none;
      /*border-radius: 0.28571429rem;*/
      text-align: left;
      color: rgba(0, 0, 0, 0.87);
      border-collapse: separate;
      border-spacing: 0px;
      padding: 0 15px 0 15px;
    }

    .ui.table:first-child {
      margin-top: 0em;
    }

    .ui.table:last-child {
      margin-bottom: 0em;
    }

   /* .ui.table th,
    .ui.table td {
      -webkit-transition: background 0.1s ease, color 0.1s ease;
      transition: background 0.1s ease, color 0.1s ease;
    }*/

    .ui.table thead {
      box-shadow: none;
    }

    .ui.table thead th {
      cursor: auto;
      /*background: #F9FAFB;*/
      text-align: inherit;
      color: rgba(0, 0, 0, 0.87);
      padding: 0.92857143em 0.78571429em;
      vertical-align: inherit;
      font-style: none;
      font-weight: bold;
      text-transform: none;
      border-bottom: 1px solid rgba(34, 36, 38, 0.1);
      border-left: none;
    }

/*    .ui.table thead tr > th:first-child {
      border-left: none;
    }*/

  /*  .ui.table thead tr:first-child > th:first-child {
      border-radius: 0.28571429rem 0em 0em 0em;
    }
*/
/*    .ui.table thead tr:first-child > th:last-child {
      border-radius: 0em 0.28571429rem 0em 0em;
    }*/

/*    .ui.table thead tr:first-child > th:only-child {
      border-radius: 0.28571429rem 0.28571429rem 0em 0em;
    }*/

    .ui.table tr td {
        padding-bottom: 0rem !important;
        padding-top: 0rem !important;
    }

    .ui.table tr:first-child td {
      border-top: none;
    }

    .ui.table td {
      padding: 0.78571429em 0.78571429em;
      text-align: inherit;
    }

    .hide {
        display: none;
    }
    .center {
      text-align: center;
    }
    .border-thin-left {
      border-left: thin #999 solid
    }
    .border-thin-bottom {
      border-bottom: thin #999 solid
    }
    .no-top-margin {
      margin-top: 0px !important
    }
</style>
<body>
    <center>
      <%# <img class="ui medium image" src="https://preview.ibb.co/f51hRG/Medilink_Payor.png"> %>
      <img class="medium image" src="data:image/png;base64,">
    </center>
    <div class="panel panel-default">
        <div class="panel-heading">General</div>
        <div class="panel-body">
          <table>
            <tr>
              <td width="10%">
                &nbsp;
              </td>
              <td width="10%">
                <strong>Plan Code</strong>
              </td>
              <td width="30%">
                <%= @product.code %>
              </td>
              <td width="10%">
                <strong>Plan Name</strong>
              </td>
              <td width="30%">
                <%= @product.name %>
              </td>
              <td width="10%">
                &nbsp;
              </td>
            </tr>
            <tr>
              <td>
                &nbsp;
              </td>
              <td width="10%">
                <strong>Plan Description</strong>
              </td>
              <td>
                <%= @product.description %>
              </td>
              <td width="10%">
                <strong>Plan Type</strong>
              </td>
              <td>
                <%= @product.type %>
              </td>
              <td>
                &nbsp;
              </td>
            </tr>
            <tr>
              <td>
                &nbsp;
              </td>
              <td width="10%">
                <strong>Limit Type</strong>
              </td>
              <td>
                <%= @product.limit_type %>
              </td>
              <td>
                <strong>Limit Amount</strong>
              </td>
              <td>
                <%= @product.limit_amount %>
              </td>
              <td>
                &nbsp;
              </td>
            </tr>
            <tr>
              <td>
                &nbsp;
              </td>
              <td>
                <strong>Limit Applicability</strong>
              </td>
              <td>
                <%= @product.limit_applicability %>
              </td>
              <td>
                <strong>PHIC</strong>
              </td>
              <td>
                <%= @product.phic_status %>
              </td>
              <td>
                &nbsp;
              </td>
            </tr>
            <tr>
              <td>
                &nbsp;
              </td>
              <td>
                <strong>Standard Plan</strong>
              </td>
              <td>
                <%= @product.standard_product %>
              </td>
              <td>
                &nbsp;
              </td>
              <td>
                &nbsp;
              </td>
              <td>
                &nbsp;
              </td>
            </tr>
          </table>
        </div>
    </div>

    <div class="panel panel-default">
        <div class="panel-heading">Exclusion</div>
        <div class="panel-body">
          <table class="center">
            <tr>
              <td><strong>General Exclusion</strong></td>
            </tr>
          </table>


          <table id="" class="ui striped table">
              <tr>
                <td>Exclusion Code</td>
                <td>Exclusion Name</td>
                <td>No. of Diseases</td>
                <td>No. of Procedures</td>
              </tr>
              <%= for product_exclusion <- check_exclusion_genex(Enum.sort_by(@product.product_exclusions, &(&1.exclusion.code))) do %>
                <tr>
                  <td> <%= product_exclusion.exclusion.code %> </td>
                  <td> <%= product_exclusion.exclusion.name %> </td>
                  <td> <%= Enum.count(product_exclusion.exclusion.exclusion_diseases) %> </td>
                  <td> <%= Enum.count(product_exclusion.exclusion.exclusion_procedures) %> </td>
                </tr>
              <% end %>
          </table>

          <table class="center">
            <tr>
              <td><strong>Pre Existing Condition</strong></td>
            </tr>
          </table>

          <table>
              <tr>
                <td>Exclusion Code</td>
                <td>Exclusion Name</td>
                <td>No. of Diseases</td>
                <th>Limit Type</th>
                <th>Limit Amount</th>
              </tr>
              <%= for product_exclusion <- check_pre_existing(Enum.sort_by(@product.product_exclusions, &(&1.exclusion.code))) do %>
                <tr>
                  <td> <%= product_exclusion.exclusion.code %> </td>
                  <td> <%= product_exclusion.exclusion.name %> </td>
                  <td> <%= Enum.count(product_exclusion.exclusion.exclusion_diseases) %> </td>
                  <td>
                    <%= if not is_nil(product_exclusion.product_exclusion_limit) do %>
                      <%= product_exclusion.product_exclusion_limit.limit_type %>
                    <% end %>
                  </td>
                  <td>
                    <%= load_limit_amount(product_exclusion.product_exclusion_limit) %>
                  </td>
                </tr>
              <% end %>
          </table>
        </div>
    </div>

    <div class="panel panel-default">
        <div class="panel-heading">Benefit</div>
        <div class="panel-body">
          <table>
              <tr>
                <td>Benefit Code</td>
                <td>Benefit Name</td>
                <td>Benefit Coverage</td>
                <td>Benefit Limit Coverage</td>
              </tr>
              <%= for product_benefit <- Enum.sort_by(@product.product_benefits, &(&1.benefit.code)) do %>
                <tr>
                  <td><%= product_benefit.benefit.code %></td>
                  <td><%= product_benefit.benefit.name %></td>
                  <td><%= display_coverages_index(product_benefit.benefit.benefit_coverages) %></td>
                  <td><%= display_limit_coverages_index(product_benefit.product_benefit_limits) %></td>
                </tr>
              <% end %>
          </table>
        </div>
    </div>

    <div class="panel panel-default">
        <div class="panel-heading">Facility Access</div>
        <div class="panel-body">
          <%= for product_coverage <- @product.product_coverages do %>
            <table>
              <tr>
                <td><strong><%= load_coverages_id(product_coverage.coverage_id) %></strong></td>
              </tr>
            </table>

            <table>
              <tr>
                <td width="20%">
                  Facility
                </td>
                <td width="20%">
                  <%= if product_coverage.type == "inclusion" do %>
                    <%= "Specific Facility" %>
                  <% else %>
                    <%= "All Affiliated Facility" %>
                  <% end %>
                </td>
                <td width="60%"></td>
              </tr>
              <tr>
                <td colspan="3">
                  <%= if product_coverage.type == "inclusion" do %>
                    <%= "Inclusion" %>
                  <% else %>
                    <%= "Exception" %>
                  <% end %>
                </td>
              </tr>
            </table>
            <table>
              <tr>
                <td>Facility Code</td>
                <td>Facility Name</td>
                <td>Facility Type</td>
                <td>Region</td>
                <td>Category</td>
              </tr>
              <%= for pcf <- product_coverage.product_coverage_facilities do %>
                <tr>
                  <td>
                    <a href="#" class="open href" name="view_facility" productID="<%= @product.id %>"  productFacilityID="<%= pcf.id %>">
                      <%= pcf.facility.code %>
                    </a>
                  </td>
                  <td><%= pcf.facility.name %></td>
                  <td>
                    <%= case pcf.facility.type == nil do %>
                      <%= true -> %>
                        <%= %>
                        <% false -> %>
                          <%= pcf.facility.type.text %>
                        <% end %>
                  </td>
                  <td><%= pcf.facility.region %></td>
                  <td>
                    <%= case pcf.facility.category == nil do %>
                      <%= true -> %>
                        <%= %>
                        <% false -> %>
                          <%= pcf.facility.category.text %>
                        <% end %>
                  </td>
                </tr>

              <% end %>
              <table>
                <tr>
                  <td>&nbsp;</td>
                </tr>
              </table>
            <% end %>
            </table>
        </div>
    </div>

    <div class="panel panel-default">
      <div class="panel-heading">Condition</div>
      <div class="panel-body">
        <table class="center">
          <tr>
            <td><strong>Age Eligibility</strong></td>
          </tr>
        </table>
        <table>
          <tr>
            <td width="60%"></td>
            <td width="20%">Min Age</td>
            <td width="20%">Max Age</td>
          </tr>
          <tr>
            <td>Principal</td>
            <td><%= @product.principal_min_age %>  <%= @product.principal_min_type %></td>
            <td><%= @product.principal_max_age %>  <%= @product.principal_max_type %></td>
          </tr>
          <tr>
            <td>Adult Dependents</td>
            <td><%= @product.adult_dependent_min_age %> <%= @product.adult_dependent_min_type %></td>
            <td><%= @product.adult_dependent_max_age %> <%= @product.adult_dependent_max_type %></td>
          </tr>
          <tr>
            <td>Minor Dependents</td>
            <td><%= @product.minor_dependent_min_age %> <%= @product.minor_dependent_min_type %></td>
            <td><%= @product.minor_dependent_max_age %> <%= @product.minor_dependent_max_type %></td>
          </tr>
          <tr>
            <td>Overage Dependents</td>
            <td><%= @product.overage_dependent_min_age %> <%= @product.overage_dependent_min_type %></td>
            <td><%= @product.overage_dependent_max_age %> <%= @product.overage_dependent_max_type %></td>
          </tr>
        </table>

        <br />
        <br />
        <table class="center">
          <tr>
            <td><strong>Hierarchy of Elibigle Dependents</strong></td>
          </tr>
        </table>
        <table>
          <tr>
            <td>Married Employee</td>
            <td>Single Employee</td>
            <td>Single Parent Employee</td>
          </tr>
          <tr>
            <td>
              <%= for pchoed <- @product.product_condition_hierarchy_of_eligible_dependents do %>
                <%= if pchoed.hierarchy_type == "Married Employee" do %>
                  <%= Integer.to_string(pchoed.ranking) <> ". " <> pchoed.dependent %><br />
                  <% end %>
                  <% end %>
            </td>
            <td>
              <%= for pchoed <- @product.product_condition_hierarchy_of_eligible_dependents do %>
                <%= if pchoed.hierarchy_type == "Single Employee" do %>
                  <%= Integer.to_string(pchoed.ranking) <> ". " <> pchoed.dependent %><br />
                  <% end %>
                  <% end %>
            </td>
            <td>
              <%= for pchoed <- @product.product_condition_hierarchy_of_eligible_dependents do %>
                <%= if pchoed.hierarchy_type == "Single Parent Employee" do %>
                  <%= Integer.to_string(pchoed.ranking) <> ". " <> pchoed.dependent %><br />
                  <% end %>
                  <% end %>
            </td>
          </tr>
        </table>

        <br />
        <br />
        <table class="center">
          <tr>
            <td><strong>Deductions</strong></td>
          </tr>
        </table>
        <table>
          <tr>
            <td width="60%"></td>
            <td width="20%">Network Benefits</td>
            <td width="20%">Non Network Benefits</td>
          </tr>
          <tr>
            <td>Annual Deduction</td>
            <td>
              <%= if is_nil(@product.adnb) do %>
              <% else %>
                <div class="eight wide computer six wide tablet column">
                  P <%= @product.adnb %>  Per Year
                </div>
              <% end %>
            </td>
            <td>
              <%= if is_nil(@product.adnnb) do %>
              <% else %>
                <div class="eight wide computer six wide tablet column">

                  P <%= @product.adnnb %> Per Year
                </div>
              <% end %>
            </td>
          </tr>
          <tr>
            <td>Out of Pocket Maximum</td>
            <td>
              <%= if is_nil(@product.opmnb) do %>
              <% else %>
                <div class="eight wide computer six wide tablet column">
                  P <%= @product.opmnb %>  Per Year
                </div>
              <% end %>
            </td>
            <td>
              <%= if is_nil(@product.opmnnb) do %>
              <% else %>
                <div class="eight wide computer six wide tablet column">
                  P <%= @product.opmnnb %> Per Year
                </div>
              <% end %>
            </td>
          </tr>
        </table>

        <br />
        <br />
        <table class="center">
          <tr>
            <td><strong>Room and Board</strong></td>
          </tr>
        </table>
        <table>
          <tr>
            <td>Room and Board</td>
            <td>Room Type</td>
            <td>Room Limit Amount</td>
            <td>Room Upgrade</td>
          </tr>
          <%= for product_coverage <- @product.product_coverages do %>
            <%= if is_nil(product_coverage.product_coverage_room_and_board) do %>
              <!-- do nothing -->
            <% else %>
              <tr>
                <td>
                  <%= product_coverage.product_coverage_room_and_board.room_and_board %>
                </td>
                <td>
                  <%= preload_room_type(product_coverage.product_coverage_room_and_board.room_type) %>
                </td>
                <td>
                  <%= product_coverage.product_coverage_room_and_board.room_limit_amount %>
                </td>
                <td>
                  <%= product_coverage.product_coverage_room_and_board.room_upgrade %> <%= product_coverage.product_coverage_room_and_board.room_upgrade_time %>
                </td>
              </tr>
            <% end %>
          <% end %>
        </table>

        <br />
        <br />
        <table class="center">
          <tr>
            <td><strong>Funding Arrangment</strong></td>
          </tr>
        </table>
        <table>
          <tr>
            <td>Coverage</td>
            <td>Funding Arrangement</td>
          </tr>
          <%= for product_coverage <- Enum.sort_by(@product.product_coverages, &(&1.coverage.description)) do %>
            <tr>
              <td>
                <%= product_coverage.coverage.description %>
              </td>
              <td>
                <%= product_coverage.funding_arrangement %>
              </td>
            </tr>
          <% end %>
        </table>
        <table class="center">
          <tr>
            <td><strong>Limit Threshold</strong></td>
          </tr>
        </table>
        <%= if @product.product_coverages != nil do %>
          <%= for pc <- @product.product_coverages do %>
            <table>
              <tr>
                <td><%= pc.coverage.name %></td>
              </tr>
              <tr>
                <td>
                  <%= if not is_nil(pc.product_coverage_limit_threshold) do %>
                    <table>
                      <tr>
                        <td>Limit Threshold: <%= pc.product_coverage_limit_threshold.limit_threshold %></td>
                      </tr>
                      <tr>
                        <td>
                          Exempted Facilities: <br/>
                          <table class="ui celled padded table">
                            <tr>
                              <th>Facility</th>
                              <th>Limit Threshold</th>
                            </tr>
                            <%= for pcltf <- pc.product_coverage_limit_threshold.product_coverage_limit_threshold_facilities do %>
                              <tr>
                                <td><%= pcltf.facility.name %></td>
                                <td>P <%= pcltf.limit_threshold %></td>
                              </tr>
                            <% end %>
                          </table>
                        </td>
                      </tr>
                    </table>
                    <% else %>
                    No Limit Threshold.
                  <% end %>
                </td>
              </tr>
            </table>
          <% end %>
        <% end %>


      </div>
    </div>

    <div class="panel panel-default">
      <div class="panel-heading">Risk Share</div>
      <div class="panel-body">

        <%= for product_coverage <- Enum.sort(@product.product_coverages) do %>
          <br />
          <br />
          <table>
            <tr>
              <td><strong><%= load_coverages_id(product_coverage.coverage_id) %></strong></td>
            </tr>
          </table>
          <table>
            <tr>
              <td width="50%">
                <% product_risk_share = product_coverage.product_coverage_risk_share %>
                <%= if product_risk_share.product_coverage_id == product_coverage.id do %>
                  <br />
                  <table class="no-top-margin">
                    <tr>
                      <td><strong>Accredited Facilities</strong></td>
                    </tr>
                  </table>
                  <table>
                    <tr>
                      <td width="50%">Risk Share</td>
                      <td width="50%">
                        <%= product_risk_share.af_type %>
                      </td>
                    </tr>

                    <tr>
                      <td>Value</td>
                      <td>
                        <%= product_risk_share.af_value %>
                      </td>
                    </tr>

                    <tr>
                      <td>Covered After Risk Share</td>
                      <td>
                        <%= if product_risk_share.af_type == "N/A" or product_risk_share.af_type == "CoInsurance" do %>
                          <%= product_risk_share.af_covered_percentage %>
                          <% else %>
                          <%= product_risk_share.af_covered_amount %>
                        <% end %>
                      </td>
                    </tr>
                    <tr>
                      <td>&nbsp;</td>
                      <td>&nbsp;</td>
                    </tr>
                  </table>
              </td>
              <td width="50%">
                <br />
                <table class="no-top-margin border-thin-left">
                  <tr>
                    <td><strong>Non-Accredited Facilities</strong></td>
                  </tr>
                </table>
                <table class="border-thin-left">
                  <tr>
                    <td width="50%">Reimbursable</td>
                    <td width="50%">
                      <%= product_risk_share.naf_reimbursable %>
                    </td>
                  </tr>
                  <tr>
                    <td>Risk Share</td>
                    <td>
                      <%= product_risk_share.naf_type %>
                    </td>
                  </tr>
                  <tr>
                    <td>Value</td>
                    <td>
                      <%= product_risk_share.naf_value %>
                    </td>
                  </tr>
                  <tr>
                    <td>Covered After Risk Share</td>
                    <td>
                      <%= if product_risk_share.naf_type == "N/A" or product_risk_share.naf_type == "CoInsurance" do %>
                        <%= product_risk_share.naf_covered_percentage %>
                        <% else %>
                        <%= product_risk_share.naf_covered_amount %>
                      <% end %>
                    </td>
                  </tr>
                </table>
              <% end %>
              </td>
            </tr>
            <tr>
              <td colspan="2">
                <hr width="100%" />
              </td>
            </tr>
            <tr>
              <td colspan="2">

                <br />
                <table>
                  <tr>
                    <td><strong>Exempted Facilities</strong></td>
                  </tr>
                </table>

                <%= for product_risk_share_facility <- product_risk_share.product_coverage_risk_share_facilities do %>
                  <table>
                    <tr>
                      <td width="20%"><strong>Facility Code</strong></td>
                      <td width="20%"><strong>Facility Name</strong></td>
                      <td width="20%"><strong>Risk Share</strong></td>
                      <td width="20%"><strong>Value</strong></td>
                      <td width="20%"><strong>Covered After Risk Share</strong></td>
                    </tr>
                    <tr>
                      <td>
                        <%= product_risk_share_facility.facility.code %>
                      </td>
                      <td>
                        <%= product_risk_share_facility.facility.name %>
                      </td>
                      <td>
                        <%= product_risk_share_facility.type %>
                      </td>
                      <td>
                        <%= if product_risk_share_facility.value != nil do %>
                          Value:  <%= product_risk_share_facility.value %>
                        <% else %>
                          <%= if product_risk_share_facility.type == "Copayment" do %>
                            Value: P <span class="card_mask_value"> <%= product_risk_share_facility.value_amount %> </span>
                          <% else %>
                            Value:  <%= product_risk_share_facility.value_percentage %> %
                          <% end %>
                        <% end %>
                      </td>
                      <td>
                        <%= product_risk_share_facility.covered %>
                      </td>
                    </tr>
                    <tr>
                      <td colspan="1">&nbsp;</td>
                      <td colspan="3">


                        <br />
                        <table>
                          <tr>
                            <td><strong>Exempted Procedures</strong></td>
                          </tr>
                        </table>
                        <table>
                          <tr>
                            <td width="20%">Procedure Code</td>
                            <td width="20%">Procedure Name</td>
                            <td width="20%">Risk Share</td>
                            <td width="20%">Value</td>
                            <td width="20%">Covered after Risk Share</td>
                          </tr>
                          <%= for product_risk_share_facility_procedure <- product_risk_share_facility.product_coverage_risk_share_facility_payor_procedures do %>
                            <tr>
                              <td><%= product_risk_share_facility_procedure.facility_payor_procedure.code %></td>
                              <td><%= product_risk_share_facility_procedure.facility_payor_procedure.name %></td>
                              <td><%= product_risk_share_facility_procedure.type %></td>
                              <td><%= product_risk_share_facility_procedure.value %></td>
                              <td><%= product_risk_share_facility_procedure.covered %></td>
                            </tr>
                          <% end %>
                        </table>

                      </td>
                      <td colspan="1">&nbsp;</td>
                    </tr>
                  </table>
                  <br/ >
                  <br/ >
                <% end %>

              </td>
            </tr>
            <tr>
              <td>
              </td>
            </tr>
            <tr>
              <td>
              </td>
            </tr>
          </table>
        <% end %>
      </div>
    </div>

</body>
