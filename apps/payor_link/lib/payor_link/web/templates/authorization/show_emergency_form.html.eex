<%= form_for @changeset, @action, [as: :authorization, multipart: true, id: "emergency_form"], fn f -> %>
  <input type="hidden" name="_csrf_token" value="<%= Plug.CSRFProtection.get_csrf_token() %>">
  <div class="ui grid">
    <div class="eleven wide computer eleven wide tablet column">
      <div class="ui segments">
        <div class="ui segment">
          <h3 class="ui dividing center aligned header">Emergency
          <%= if String.downcase(@authorization.status) == "for approval" do %>
          <a href="#" class="ui top right floated button" style="padding: 10px 20px !important;" id="btn_disapprove_loa" authorization_id="<%= @authorization.id %>">
            Disapprove
          </a>
          <% end %>
          <%= if String.downcase(@authorization.status) == "cancelled" do %>
          <a href="#" class="ui top right floated button" style="padding: 10px 20px !important;" id="btn_disapprove_loa" authorization_id="<%= @authorization.id %>">
            Disapprove
          </a>
          <% end %>
          </h3>
          <p id="no_procedure_ruv"></p>
          <%= hidden_input f, :id, value: @authorization.id %>
          <%= hidden_input f, :facility_id, value: @authorization.facility_id %>
          <%= hidden_input f, :sos, id: "sos" %>
          <%# ADMISSION AND DISCHARGE DATE %>
          <div class="sixteen wide column">
            <div class="two fields">
              <div class="field" id="dateFormPicker">
                <%= label f, :admission_datetime, "Admission Date" %>
                <div class="ui calendar">
                  <div class="ui input right icon">
                    <i class="calendar icon"></i>
                      <%= text_input f, :admission_datetime, value: transform_date(Ecto.Date.utc()), readonly: true %>
                  </div>
                </div>
              </div>
              <div class="field" id="dateFormPicker">
                <%= label f, :discharge_datetime, "Discharge Date" %>
                <div class="ui calendar">
                  <div class="ui input right icon">
                    <i class="calendar icon"></i>
                      <%= text_input f, :discharge_datetime, value: transform_date(Ecto.Date.utc()), readonly: true %>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <%# END OF ADMISSION AND DISCHARGE DATE %>

          <div class="sixteen wide column"></div>
          <div class="sixteen wide column field">
            <label>Chief Complaint</label>
            <%= select f, :chief_complaint, ["Fever": "Fever","Cough and Colds": "Cough and Colds","Accidents and Trauma": "Accidents and Trauma","Abdominal pain": "Abdominal pain","High blood pressure": "High blood pressure","Chest pain and palpitations": "Chest pain and palpitations", "Dizziness": "Dizziness","Back pain and Low back pain": "Back pain and Low back pain","Rashes, allergy and itchiness": "Rashes, allergy and itchiness","Headache": "Headache","Others": "Others"], class: "ui search selection dropdown search-dropdown disabled", prompt: "" %>
            <input type="text" id="others" name="authorization[chief_complaint_others]" style="display: none" value="<%= @authorization.chief_complaint_others %>" %>
          </div>

          <%# ADD PRACTITIONER %>
          <div class="ui grid">
            <div class="fourteen wide five column">
              <div class="column">
                <div class="ui mini label" style="visibility: hidden">Hidden</div>
              </div>
              <div class="field column">
                <label>Practitioner</label>
              </div>
            </div>
            <div class="two wide column">
              <div class="column">
                <label style="visibility:hidden;">Hidden</label>
              </div>
              <div class="column">
                <div class="ui right floated primary small add button" style="visibility: hidden"  id="add_practitioner">
                  <i class="plus icon"></i>
                  Add
                </div>
              </div>
            </div>
          </div>
          <table id="tblauthorization_procedure" class="ui stripped table">
            <thead>
              <tr>
                <th style="text-align: center;">Practitioner Name</th>
                <th style="text-align: center;">Specialization</th>
                <th style="text-align: center;">Role</th>
                <th style="text-align: center;">Practitioner's Rate</th>
                <th style="text-align: center;">Action</th>
              </tr>
            </thead>
            <tbody>
              <%= if @authorization.authorization_practitioner_specializations == [] do %>
                <tr>
                  <td colspan="7" class="center aligned">
                    NO RECORD FOUND!
                  </td>
                </tr>
              <% else %>
                <%= for aps <-  @authorization.authorization_practitioner_specializations do %>
                  <tr>
                    <td width="25%" style="text-align: center;"> <%= aps.practitioner_specialization.practitioner.first_name %> <%= aps.practitioner_specialization.practitioner.last_name %> </a></td>
                    <td width="25%" style="text-align: center;"> <%= aps.practitioner_specialization.specialization.name %></td>
                    <td width="20%" style="text-align: center;"> <%= aps.role %></td>
                    <td width="15%" style="text-align: center;"> 0 </td>
                    <td width="15%" style="text-align: center;" class="ui center aligned">
                      <button type="button" class="ui floated blue button icon edit_aps" practitioner="<%= aps.practitioner_specialization.practitioner.id %>" specialization="<%= aps.practitioner_specialization.specialization.id %>" role="<%= aps.role %>" aps_id="<%= aps.id %>">
                        <i class="edit icon" ></i>
                      </button>
                      <button type="button" class="ui floated red button icon remove_aps" a_id="<%= @authorization.id %>" aps_id="<%= aps.id %>">
                        <i class="trash icon" ></i>
                      </button>
                    </td>
                  </tr>
                <% end %>
              <% end %>
            </tbody>
          </table>

          <%# ADD DISEASE PROCEDURE %>
          <div class="ui grid">
            <div class="fourteen wide five column">
              <div class="column">
                <div class="ui mini label" style="visibility: hidden">Hidden</div>
              </div>
              <div class="field column">
                <label>Disease/Procedure</label>
              </div>
            </div>
            <div class="two wide column">
              <div class="column">
                <label style="visibility:hidden;">Hidden</label>
              </div>
              <div class="column">
                <%= if Enum.empty?(@authorization.authorization_practitioner_specializations) do %>
                  <div class="ui right floated primary small add button" style="visibility: hidden" id="add_dp">
                <% else %>
                  <div class="ui right floated primary small add button" id="add_dp">
                <% end %>
                  <i class="plus icon"></i>
                  Add
                </div>
              </div>
            </div>
          </div>
          <table id="tblauthorization_procedure" class="ui stripped table">
            <thead>
              <tr>
                <th style="text-align: center;">Disease</th>
                <th style="text-align: center;">Procedure</th>
                <th style="text-align: center;">Unit</th>
                <th style="text-align: center;">Amount</th>
                <th style="text-align: center;">Practitioner</th>
                <th style="text-align: center;">Action</th>
              </tr>
            </thead>
            <tbody>
              <%= if @authorization_procedures == [] do %>
                <tr>
                  <td colspan="7" class="center aligned">
                    NO RECORD FOUND!
                  </td>
                </tr>
              <% else %>
                <%= for authorization_procedure <-  @authorization_procedures do %>
                  <tr>
                    <td width="25%" style="text-align: center;"> <%= authorization_procedure.diagnosis.code <> " | " <> authorization_procedure.diagnosis.description %> </a></td>
                    <td width="20%" style="text-align: center;"> <%= authorization_procedure.payor_procedure.code <> " | " <> authorization_procedure.payor_procedure.description %></td>
                    <td width="10%" style="text-align: center;"> <%= authorization_procedure.unit %></td>
                    <td width="10%" style="text-align: center;"> <%= Decimal.mult(authorization_procedure.amount, authorization_procedure.unit) %></td>
                    <td width="20%" style="text-align: center;"><%= authorization_procedure.authorization_practitioner_specialization.practitioner_specialization.practitioner.first_name %>  <%= authorization_procedure.authorization_practitioner_specialization.practitioner_specialization.practitioner.last_name %></td>
                    <td width="15%" style="text-align: center;" class="ui center aligned">
                      <button type="button" class="ui floated blue button icon edit_adp" diagnosis="<%= authorization_procedure.diagnosis.code %> | <%= authorization_procedure.diagnosis.description %>" procedure="<%= authorization_procedure.payor_procedure.code %> | <%= authorization_procedure.payor_procedure.description %>" unit="<%= authorization_procedure.unit %>" amount="<%= Decimal.mult(authorization_procedure.amount, authorization_procedure.unit) %>" practitioner="<%= authorization_procedure.authorization_practitioner_specialization.practitioner_specialization.practitioner.first_name %>  <%= authorization_procedure.authorization_practitioner_specialization.practitioner_specialization.practitioner.last_name %>" procedure_id="<%= authorization_procedure.payor_procedure.id %>" aid="<%= authorization_procedure.id %>">
                        <i class="edit icon" ></i>
                      </button>
                      <button type="button" class="ui floated red button icon btnRemoveProcedure" authorization_procedure_id="<%= authorization_procedure.id %>">
                        <i class="trash icon" ></i>
                      </button>

                    </td>
                  </tr>
                <% end %>
              <% end %>
            </tbody>
          </table>

        <%# SENIOR CITIZEN/PWD %>
          <br />
          <div class="ui grid">
            <div class="three wide column">
              <div class="ui checkbox disabled">
                <%= if @authorization.member.senior or age(@authorization.member.birthdate) >= 60 do %>
                  <%= checkbox(f, :senior, id: "senior", checked: true) %>
                <% else %>
                  <%= checkbox(f, :senior, id: "senior") %>
                <% end %>
                  <label>Senior Citizen</label>
              </div>
            </div>
            <div class="two wide column">
              <div class="ui checkbox disabled">
                <%= if @authorization.member.pwd do %>
                  <%= checkbox(f, :pwd, id: "pwd", checked: true) %>
                <% else %>
                  <%= checkbox(f, :pwd, id: "pwd") %>
                <% end %>
                <label>PWD</label>
              </div>
            </div>
            <div class="eleven wide column"></div>
          </div>

          <div class="ui grid">
            <div class="eight wide column" id="senior_id" style="display:none">
              <div class="field disabled">
                <label>Senior Citizen ID</label>
                <%= if @authorization.member.senior do %>
                  <%= text_input f, :senior_citizen_id, value: @authorization.member.senior_id %>
                <% else %>
                  <%= text_input f, :senior_citizen_id %>
                <% end %>
              </div>
            </div>
            <div class="eight wide column" id="senior_discount" style="display:none">
              <div class="field disabled" id="senior_field">
                <label>Senior Citizen Discount</label>
                <%= text_input f, :senior_discount, id: "senior_text" %>
              </div>
            </div>
            <div class="eight wide column" id="pwd_id" style="display:none">
              <div class="field">
                <label>PWD ID</label>
                <%= if @authorization.member.pwd do %>
                  <%= text_input f, :pwd_id, value: @authorization.member.pwd_id %>
                <% else %>
                  <%= text_input f, :pwd_id %>
                <% end %>
              </div>
            </div>
            <div class="eight wide column" id="pwd_discount" style="display:none">
              <div class="field" id="pwd_field">
                <label>PWD Discount</label>
                <%= text_input f, :pwd_discount, id: "pwd_text" %>
              </div>
            </div>
            <div class="ui calendar eight wide column" id="date_issued" style="display:none" >
              <div class="field">
                <label>Date Issued</label>
                 <div class="ui fluid input right icon">
                  <i class="calendar icon"></i>
                  <%= text_input f, :date_issued %>
                </div>
              </div>
            </div>
            <div class="eight wide column" id="place_issued" style="display:none">
              <div class="field">
                <label>Place Issued</label>
                <%= text_input f, :place_issued %>
              </div>
            </div>
            <div class="nine wide column">
              <div class="field">
                <label>Pharmacy</label>
                <label>Total Amount</label>
                <div class="ui right labeled input">
                  <%= text_input f, :date_issued, disabled: true %>
                  <div class="ui basic label">
                    PHP
                  </div>
                </div>
              </div>
            </div>
            <div class="seven wide column">
              <div class="field">
                <label style="visibility:hidden;">Hidden</label><label style="visibility:hidden;">Hidden</label>
                <button class="ui primary button disabled" style="visibility: hidden" >i<i class="add icon"></i>Add Details</button>
                <button class="ui blue button" style="visibility: hidden">View Details</button>
              </div>
            </div>
            <div class="nine wide column">
              <div class="field">
                <label>Miscellaneous Item</label>
                <label>Total Amount</label>
                <div class="ui right labeled input">
                  <%= text_input f, :date_issued, disabled: true %>
                  <div class="ui basic label">
                    PHP
                  </div>
                </div>
              </div>
            </div>
            <div class="seven wide column">
              <div class="field">
                <label style="visibility:hidden;">Hidden</label><label style="visibility:hidden;">Hidden</label>
                <button class="ui primary button disabled" style="visibility: hidden" ><i class="add icon"></i>Add Details</button>
                <button class="ui blue button" style="visibility: hidden">View Details</button>
              </div>
            </div>
            <div class="nine wide column">
              <div class="field">
                <label>Operating and Delivery Room Fees</label>
                <div class="ui right labeled input">
                  <%= text_input f, :date_issued, disabled: true %>
                  <div class="ui basic label">
                    PHP
                  </div>
                </div>
              </div>
            </div>
            <div class="seven wide column"></div>
          </div>

          <div class="ui grid">
            <div class="sixteen wide column">
               <div class="field" role="LOAFileUpload">
                  <label>Practitioner Prescription</label>
                <div class="ui action input">
                  <input type="text" readonly="true">
                  <button class="ui labeled icon button disabled" style="visibility: hidden"  type="button" id="addFile">
                    <i class="upload icon"></i>
                      Browse Document
                  </button>
                </div>
                </div>
              </div>
              <div class="sixteen wide column">
                <div class="field">
                <%= hidden_input f, :file_delete_ids, id: "deleteIDs", value: "" %>
                  <div class="field">
                    <div class="ui middle aligned list" id="filePreviewContainer">

                      </div>
                    </div>
                </div>
            </div>
          </div>

          <div class="field">
            <div class="ui checkbox">
              <%= checkbox(f, :member_pay, disabled: true) %>
                <label>Member will pay for member excess</label>
            </div>
          </div>
          <div class="field">
            <label>Special Approval</label>
            <%= select f, :special_approval_id, Enum.map(@special_approval, &{&1.text, &1.id}), prompt: "Special Approval", class: "ui selection dropdown disabled" %>
          </div>

          <div class="field">
            <label>Internal Remarks</label>
            <%= if @authorization.status == "Disapproved" do %>
              <%= textarea f, :internal_remarks, class: "form-control", readonly: true, value: "Reason for LOA Disapproval: #{@authorization.reason}" %>
            <%= else %>
              <%= textarea f, :internal_remarks, class: "form-control", readonly: true %>
            <% end %>
            <%# <font color="red"><%= error_tag f, :internal_remarks %1></font> %>
          </div>

<%#           <button type="button" class="ui button mrg1T" id="save"> %>
<%#             <i class="save icon"></i> %>
<%#             Save %>
<%#           </button> %>

<%#           <1%# SAVE/SUBMIT/CANCEL BUTTON %1> %>
<%#           <%= if Enum.count(@authorization.authorization_facility_ruvs) == 0 and Enum.count(@authorization_procedures) == 0 do %1> %>
<%#             <button type="button" id="invalid_emergency_submit" class="ui primary floated button mrg1T"> %>
<%#               <i class="send icon"></i> %>
<%#               Submit %>
<%#             </button> %>
<%#           <% else %1> %>
<%#             <button id="emergency_submit" type="button" class="ui primary floated button mrg1T"> %>
<%#               <i class="send icon"></i> %>
<%#               Submit %>
<%#             </button> %>
<%#           <% end %1> %>
<%#           <button type="button" class="ui red right floated button mrg1T"> %>
<%#             <i class="close icon"></i> %>
<%#             Cancel %>
<%#           </button> %>

   <%= if @authorization.status == "Approved" do %>
     <a href="#" id="" class="ui default button mrg1T">
       <i class="print icon"></i>
       Print
     </a>
     <% can_resched = Date.compare(@authorization.valid_until, Ecto.Date.utc()) == :lt %>
     <%= if can_resched do %>
       <div id="" class="ui blue left button mrg1T">
         <i class="calendar icon"></i>
         Reschedule LOA
       </div>
     <% end %>
     <%= end %>

     <%= if @authorization.status == "For Approval" do %>
       <div class="ui green  button mrg1T" id="btn_approve_loa" authorization_id="<%= @authorization.id %>">
        Approve
      </div>
       <a class="ui blue right floated button mrg1T" id="">
         <i class="edit icon"></i>
         Edit
       </a>
     <% end %>

   <%= if @authorization.status == "OTP Verified" do %>
     <a href="#" id="" class="ui default button mrg1T">
       <i class="print icon"></i>
       Print
     </a>
     <%= end %>


        </div>
      </div>
    </div>
    <div class="five wide computer eleven wide tablet column">
      <div class="ui styled fluid accordion">
        <div class="title">
          <h3>
            Member
            <i class="dropdown icon" style="float: right"></i>
          </h3>
        </div>
        <div class="content">
          <div class="ui grid">
            <div class="eight wide computer eight wide computer column">
              <label>Name</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label><%= member_full_name(@member) %></label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Card Number</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label><%= @member.card_no %></label>
            </div>
            <%# <div class="eight wide computer eight wide computer column"> %>
            <%#   <label>Member ID</label> %>
            <%# </div> %>
            <%# <div class="eight wide computer eight wide computer column"> %>
            <%#   <label><%= @member.id %1></label> %>
            <%# </div> %>
            <div class="eight wide computer eight wide computer column">
              <label>Birth Date</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label id="birth_date"><%= format_birthdate(@member.birthdate) %></label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Age</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label id="age">
               <%= get_age(@member.birthdate) %>
              </label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Gender</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label><%= @member.gender %></label>
            </div>
          </div>
        </div>
      </div>

      <div class="ui styled fluid accordion">
        <div class="title">
          <h3>
            Facility
            <i class="dropdown icon" style="float: right"></i>
          </h3>
        </div>
        <div class="content">
          <div class="ui grid">
            <div class="eight wide computer eight wide computer column">
              <label>Facility Code</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label><%= @authorization.facility.code %></label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Facility Name</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label><%= @authorization.facility.name %> </label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Facility Type</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label><%= @authorization.facility.type.text %> </label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Facility Category</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label><%= @authorization.facility.category.text %> </label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Address</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label id="facility_address">
                <%= @authorization.facility.line_1 %>  <%= @authorization.facility.line_2 %>,
                <%= @authorization.facility.city %>  <%= @authorization.facility.province %>
                <%= @authorization.facility.region %>, <%= @authorization.facility.country %>, <%= @authorization.facility.postal_code %>
              </label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Contact Number</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <%= cond do %>
              <% not is_nil(@authorization.facility.phone_no) -> %>
                  <label><%= @authorization.facility.phone_no %></label>
              <% Enum.empty?(@facility_contacts) == false -> %>
                <label><%= Enum.at(Enum.at(@facility_contacts, 0).phones, 0).number %></label>
              <% true -> %>
                <label> N/A </label>
              <% end %>
            </div>
          </div>
        </div>
      </div>

      <h3>Details</h3>
      <div class="ui segments">
        <div class="ui segment">
          <div class="ui grid">
            <div class="eight wide computer eight wide computer column">
              <label>Coverage</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label><%= @authorization.coverage.name %></label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Date Created</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label id="date_created"></label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label>Valid until</label>
            </div>
            <div class="eight wide computer eight wide computer column">
              <label id="valid_until"></label>
            </div>
          </div>
        </div>
      </div>

<h3>Fees</h3>
<div class="ui segments">
  <%# PHILHEALTH PAYS %>
  <div class="ui styled fluid accordion">
    <div class="title">
      <h5>
        PhilHealth Pays (Php)
        <i class="dropdown icon" style="float: right"></i>
        <p style="float: right"> <%= display_authorization_amounts(@authorization, :company_covered) %> </p>
      </h5>
    </div>
    <div class="content">
      <div class="accordion transition hidden">
      <%# DIAGNOSIS PHILHEALTH PAYS%>
        <%= if Enum.count(filter_philhealth_pays_without_product(@authorization)) >= 1 do %>
          <div class="title">
            <i class="dropdown icon"></i>
            Not Part of Member's Product
            <p style="float: right"> <%= get_total_philpays_without_product(@authorization) %> </p>
          </div>
          <%# CONTENT %>
            <div class="content">
              <div class="ui segments">
                <div class="ui segment">
                  <div class="ui grid">
                   <%= for diagnosis_procedures <- filter_philhealth_pays_without_product(@authorization) do %>
                    <% diagnosis_procedures_list = [diagnosis_procedures] %>
                    <%= for _diagnosis_procedure <- diagnosis_procedures_list do %>
                      <% diagnosis_codes = Enum.group_by(diagnosis_procedures_list, &(&1.diagnosis.code)) %>
                      <%= for {diagnosis_code, diagnosis_procedures2} <- diagnosis_codes do %>
                        <div class="right floated six wide computer column">
                          <b> <%= diagnosis_code %> </b>
                        </div>
                        <div class="right aligned right floated six wide computer column">
                        </div>
                        <div class="right aligned right floated four wide computer column">
                          <label>100%</label>
                        </div>
                        <%= for diagnosis_procedure3 <- diagnosis_procedures2 do %>
                          <div class="right aligned right floated six wide computer column">
                          </div>
                          <div class="right floated six wide computer column">
                            <b> <%= diagnosis_procedure3.payor_procedure.code %> </b>
                          </div>
                          <div class="right aligned right floated four wide computer column">
                            <%= diagnosis_procedure3.philhealth_pay %>
                          </div>
                          <% end %>
                          <% end %>
                          <% end %>
                          <% end %>
                          <div class="sixteen wide computer column">
                            <h5 class="ui dividing center aligned header"></h5>
                          </div>

                          <div class="right aligned right floated six wide computer column">
                          </div>
                          <div class="right floated six wide computer column">
                            <b> <label>TOTAL</label> </b>
                          </div>
                          <div class="right aligned right floated four wide computer column">
                            <%= get_total_philpays_without_product(@authorization) %>
                          </div>
                  </div>
                </div>
              </div>
            </div>
