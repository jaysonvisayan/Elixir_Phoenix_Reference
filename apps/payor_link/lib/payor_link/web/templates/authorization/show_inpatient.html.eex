<%= render "breadcrumbs_new.html" %>
<div class="ui equal width left aligned padded grid stackable">
  <div class="row">
    <div class="column">
      <div class="ui segments" name="formValidateStep4Inpatient">
        <div class="ui form segment">

          <%= if @changeset.action do %>
            <div class="ui negative message">
              <i class="close icon"></i>
              <%= get_flash(@conn, :error) %>
            </div>
          <% end %>
             <%= form_for @changeset, authorization_path(@conn, :update_setup, @authorization, step: "4"), [as: :authorization, multipart: true, id: "inpatient_form"], fn f -> %>
            <input type="hidden" name="_csrf_token" value="<%= Plug.CSRFProtection.get_csrf_token() %>">
            <div class="ui grid" id="inpatient">
              <div class="eleven wide computer eleven wide tablet column">
                <div class="ui segments">
                  <div class="ui segment">
                  <h3 class="ui dividing center aligned header"> Inpatient
                    <%= if @authorization.status == "For Approval" do %>
                    <a href="#" class="ui top right floated button" style="padding: 10px 20px !important;" id="btn_disapprove_loa" authorization_id="<%= @authorization.id %>">
                      Disapprove
                    </a>
                    <% end %>
                  </h3>

                    <p id="no_procedure_ruv"></p>
                    <%= hidden_input f, :authorization_id, value: @authorization.id %>
                    <%= hidden_input f, :facility_id, value: @authorization.facility_id %>
                    <%= hidden_input f, :procedure_count, value: Enum.count(@authorization_procedures), id: "authorizationProcedureCount" %>
                    <%= hidden_input f, :ruv_count, value: Enum.count(@authorization.authorization_facility_ruvs), id: "authorizationRUVCount" %>
                    <%= hidden_input f, :start_date, value: Ecto.Date.utc() %>
                    <%# <% auth_stat_check = if @authorization.status == "Draft", do: nil, else: "readonly" %1> %>
                    <%# <% auth_stat_check = if @authorization.status == "Draft", do: nil, else: "disable" %1> %>

                    <div class="fields">
                      <div class="six wide field" id="dateFormPicker">
                        <%= label f, :authorization_start_date, "Admission Date" %>
                        <div class="ui calendar" id="ip_admission_date">
                          <div class="ui input right icon">
                            <i class="calendar icon"></i>
                            <input name="authorization[admission_datetime]" type="text" disabled>
                          </div>
                        </div>
                      </div>
                      <div class="seven wide field" id="dateFormPicker">
                        <%= label f, :authorization_discharge_date, "Estimated Discharge Date" %>
                        <div class="ui calendar" id="ip_discharge_date">
                          <div class="ui input right icon">
                            <i class="calendar icon"></i>
                            <input name="authorization[discharge_datetime]" type="text" disabled>
                          </div>
                        </div>
                      </div>
                      <div class="three wide field">
                        <%= label f, :length_of_stay, "Length of Stay" %>
                        <input name="authorization[length_of_stay]" type="text" readonly="true" id="lengthOfStay" disabled>
                      </div>
                    </div>

                    <div class="fields">
                      <div class="six wide field">
                        <%= label f, :nature_of_admission, "Nature of Admission" %>
                        <div class="inline fields">
                          <div class="field">
                            <div class="ui radio checkbox">
                              <%= radio_button(f, :nature_of_admission, "Emergency", checked: "", disabled: true) %>
                              <label>Emergency</label>
                            </div>
                          </div>
                          <div class="field">
                            <div class="ui radio checkbox" disabled>
                              <%= radio_button(f, :nature_of_admission, "Non-Emergency", checked: "", disabled: true) %>
                              <label>Non - Emergency</label>
                            </div>
                          </div>
                        </div>
                      </div>

                      <div class="seven wide field">
                        <%= label f, :point_of_admission, "Point of Admission" %>
                        <div class="inline fields">
                          <div class="field">
                            <div class="ui radio checkbox" disabled>
                              <%= radio_button(f, :point_of_admission, "Emergency Room", checked: "", disabled: true) %>
                              <label>Emergency Room</label>
                            </div>
                          </div>
                          <div class="field">
                            <div class="ui radio checkbox" disabled>
                              <%= radio_button(f, :point_of_admission, "Admitting Department", checked: "", disabled: true) %>
                              <label>Admitting Department</label>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>

                    <%# ADD ROOM AND BOARD %>
                    <div class="ui grid">
                      <div class="fourteen wide five column">
                        <div class="column">
                          <label style="visibility:hidden;">Hidden</label>
                        </div>
                        <div class="column">
                          <div class="ui mini label" style="visibility: hidden">Hidden</div>
                        </div>
                        <div class="field column">
                          <label>Room and Board</label>
                        </div>
                      </div>
                      <div class="two wide column">
                        <div class="column">
                          <label style="visibility:hidden;">Hidden</label>
                          </div>              <div class="column">
                          <label style="visibility:hidden;">Hidden</label>
                        </div>
                        <div class="column">
                          <div class="ui right floated primary small add button disabled" id="btnAddRoomAndBoard" arbID=<%= get_latest_authorization_room(@authorization.id) %>>
                            <i class="plus icon"></i>
                            Add
                          </div>
                        </div>
                      </div>
                    </div>
                    <table id="tblauthorization_procedure" class="ui stripped table">
                      <thead>
                        <tr>
                          <th width="10%" style="text-align: center;">Admission Date</th>
                          <th width="10%" style="text-align: center;">Admission Time</th>
                          <th width="10%" style="text-align: center;">Transfer Date</th>
                          <th width="10%" style="text-align: center;">Transfer Time</th>
                          <th width="10%" style="text-align: center;">Room Number</th>
                          <th width="10%" style="text-align: center;">Room Category</th>
                          <th width="10%" style="text-align: center;">Room Rate</th>
                          <th width="10%" style="text-align: center;">For Isolation</th>
                          <th width="10%" style="text-align: center;">Action</th>
                        </tr>
                      </thead>
                      <tbody>
                        <%= if Enum.empty?(@authorization_rooms) do %>
                          <tr>
                            <td colspan="9" class="center aligned">
                              NO RECORD FOUND!
                            </td>
                          </tr>
                        <% else %>
                          <%= for authorization_room <-  @authorization_rooms do %>
                            <tr>
                              <td width="10%" style="text-align: center;"> <%= authorization_room.admission_date %> </a></td>
                            <td width="10%" style="text-align: center;"> <%= authorization_room.admission_time %></td>
                            <td width="10%" style="text-align: center;"> <%= authorization_room.transfer_date %></td>
                            <td width="10%" style="text-align: center;"> <%= authorization_room.transfer_time %></td>
                            <td width="10%" style="text-align: center;"> <%= authorization_room.facility_room_rate.facility_room_number %></td>
                            <td width="10%" style="text-align: center;"> <%= authorization_room.facility_room_rate.facility_room_type %></td>
                            <td width="10%" style="text-align: center;"> <%= authorization_room.facility_room_rate.facility_room_rate %></td>
                            <td width="10%" style="text-align: center;"> <%= authorization_room.for_isolation %></td>
                            <td width="10%" style="text-align: center;" class="ui center aligned">
                              <button type="button" class="ui floated blue button icon edit_arb" authorization_room="<%= authorization_room.id %>" admission_date=<%= authorization_room.admission_date %> admission_time=<%= authorization_room.admission_time %> transfer_date=<%= authorization_room.transfer_date || "" %> transfer_time=<%= authorization_room.transfer_time || "" %> room_type=<%= authorization_room.facility_room_rate.facility_room_type %> room_number=<%= authorization_room.facility_room_rate.facility_room_number %> room_rate=<%= authorization_room.facility_room_rate.facility_room_rate%>>
                                <i class="edit icon disabled" ></i>
                              </button>
                              <!-- <button type="button" class="ui floated red button icon btnRemoveRoomAndBoard" authorization_room_id="<%#= authorization_room.id %>" authorization_id=<%#= @authorization.id%>>
                                <i class="trash icon" ></i>
                              </button> -->
                            </td>
                          </tr>
                        <% end %>
                      <% end %>
                    </tbody>
                  </table>

                  <%# ADD PRACTITIONER %>
                  <div class="ui grid">
                    <div class="fourteen wide five column">
                      <div class="column">
                        <label style="visibility:hidden;">Hidden</label>
                      </div>
                      <div class="column">
                        <div class="ui mini label" style="visibility: hidden">Hidden</div>
                      </div>
                      <div class="field column">
                        <label>Practitioner</label>
                      </div>
                    </div>
                    <div class="two wide column">
                      <div class="column">
                        <label style="visibility:hidden;">Hidden</label>
                        </div>              <div class="column">
                        <label style="visibility:hidden;">Hidden</label>
                      </div>
                      <div class="column">
                        <div class="ui right floated primary small add button disabled" id="btnAddPractitioner">
                          <i class="plus icon"></i>
                          Add
                        </div>
                      </div>
                    </div>
                  </div>
                  <table id="tblauthorization_procedure" class="ui stripped table">
                    <thead>
                      <tr>
                        <th width="30%" style="text-align: center;">Practitioner Name</th>
                        <th width="30%" style="text-align: center;">Specialization</th>
                        <th width="10%" style="text-align: center;">Role</th>
                        <th width="10%" style="text-align: center;">Practitioner Rate</th>
                        <th width="10%" style="text-align: center;">Action</th>
                      </tr>
                    </thead>
                    <tbody>
                      <%= if Enum.empty?(@authorization_procedures) do %>
                        <tr>
                          <td colspan="5" class="center aligned">
                            NO RECORD FOUND!
                          </td>
                        </tr>
                      <% else %>
                        <%= for authorization_procedure <-  @authorization_procedures do %>
                          <tr>
                            <td width="10%" style="text-align: center;"> <%= authorization_procedure.diagnosis.code <> " | " <> authorization_procedure.diagnosis.description %> </a></td>
                          <td width="25%" style="text-align: center;"> <%= authorization_procedure.payor_procedure.code <> " | " <> authorization_procedure.payor_procedure.description %></td>
                          <td width="10%" style="text-align: center;"> <%= authorization_procedure.unit %></td>
                          <td width="25%" style="text-align: center;"> <%= Decimal.mult(authorization_procedure.amount, authorization_procedure.unit) %></td>
                          <td width="10%" style="text-align: center;"><%= authorization_procedure.philhealth_pay || 0 %> </td>
                          <td width="5%" style="text-align: center;" class="ui center aligned">
                            <button type="button" class="ui floated red button icon btnRemoveProcedure" authorization_procedure_id="<%= authorization_procedure.id %>">
                              <i class="trash icon" ></i>
                            </button>
                          </td>
                        </tr>
                      <% end %>
                    <% end %>
                  </tbody>
                </table>

                <%# ADD DISEASE PROCEDURE %>
                <div class="ui grid">
                  <div class="fourteen wide five column">
                    <div class="column">
                      <label style="visibility:hidden;">Hidden</label>
                    </div>
                    <div class="column">
                      <div class="ui mini label" style="visibility: hidden">Hidden</div>
                    </div>
                    <div class="field column">
                      <label>Disease/Procedure</label>
                    </div>
                  </div>
                  <div class="two wide column">
                    <div class="column">
                      <label style="visibility:hidden;">Hidden</label>
                      </div>              <div class="column">
                      <label style="visibility:hidden;">Hidden</label>
                    </div>
                    <div class="column">
                      <div class="ui right floated primary small add button disabled" id="btnAddDiseaseProcedure">
                        <i class="plus icon"></i>
                        Add
                      </div>
                    </div>
                  </div>
                </div>
                <table id="tblauthorization_procedure" class="ui stripped table">
                  <thead>
                    <tr>
                      <th width="30%" style="text-align: center;">Disease</th>
                      <th width="30%" style="text-align: center;">Procedure</th>
                      <th width="10%" style="text-align: center;">Unit</th>
                      <th width="10%" style="text-align: center;">Amount</th>
                      <th width="10%" style="text-align: center;">PhilHealth</th>
                      <th width="10%" style="text-align: center;">Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <%= if Enum.empty?(@authorization_procedures) do %>
                      <tr>
                        <td colspan="7" class="center aligned">
                          NO RECORD FOUND!
                        </td>
                      </tr>
                    <% else %>
                      <%= for authorization_procedure <-  @authorization_procedures do %>
                        <tr>
                          <td width="10%" style="text-align: center;"> <%= authorization_procedure.diagnosis.code <> " | " <> authorization_procedure.diagnosis.description %> </a></td>
                        <td width="25%" style="text-align: center;"> <%= authorization_procedure.payor_procedure.code <> " | " <> authorization_procedure.payor_procedure.description %></td>
                        <td width="10%" style="text-align: center;"> <%= authorization_procedure.unit %></td>
                        <td width="25%" style="text-align: center;"> <%= Decimal.mult(authorization_procedure.amount, authorization_procedure.unit) %></td>
                        <td width="10%" style="text-align: center;"><%= authorization_procedure.philhealth_pay || 0 %> </td>
                        <td width="5%" style="text-align: center;" class="ui center aligned">
                          <button type="button" class="ui floated red button icon btnRemoveProcedure" authorization_procedure_id="<%= authorization_procedure.id %>">
                            <i class="trash icon" ></i>
                          </button>
                        </td>
                      </tr>
                    <% end %>
                  <% end %>
                </tbody>
              </table>

              <div class="field">
                <div class="inline fields">
                  <div class="field">
                    <div class="ui checkbox">
                      <input type="checkbox" name="authorization[senior_citizen]" value="Senior Citizen" disabled>
                      <label>Senior Citizen</label>
                    </div>
                  </div>
                  <div class="field">
                    <div class="ui checkbox">
                      <input type="checkbox" name="authorization[pwd]" value="PWD" disabled>
                      <label>PWD</label>
                    </div>
                  </div>
                </div>

                <div class="fields">
                  <div class="six wide field">
                    <%= label f, :senior_id, "Senior ID" %>
                    <%= text_input f, :senior_id, disabled: true %>
                  </div>
                  <div class="six wide field">
                    <%= label f, :senior_discount, "Senior Citizen Discount" %>
                    <%= text_input f, :senior_discount, disabled: true %>
                  </div>
                </div>

                <div class="fields">
                  <div class="six wide field">
                    <%= label f, :pwd_id, "PWD ID" %>
                    <%= text_input f, :pwd_id, disabled: true%>
                  </div>
                  <div class="six wide field">
                    <%= label f, :pwd_discount, "PWD Discount" %>
                    <%= text_input f, :pwd_discount, disabled: true %>
                  </div>
                </div>

                <div class="fields">
                  <div class="six wide field" id="dateFormPicker">
                    <%= label f, :date_issued, "Date Issued" %>
                    <div class="ui calendar">
                      <div class="ui input right icon">
                        <i class="calendar icon"></i>
                        <input name="authorization[date_issued]" type="text" disabled>
                      </div>
                    </div>
                  </div>
                  <div class="six wide field">
                    <%= label f, :place_issued, "Place Issued" %>
                    <%= text_input f, :place_issued, disabled: true %>
                  </div>
                </div>
              </div>

              <div class="field">
                <label>Pharmacy</label>
                <div class="fields">
                  <div class="seven wide field">
                    <%= label f, :pharmacy_total_amount, "Total Amount" %>
                    <div class="ui right labeled input">
                      <%= text_input f, :pharmacy_total_amount, disabled: true %>
                      <div class="ui label">
                        PHP
                      </div>
                    </div>
                  </div>
                  <div class="four wide field">
                    <label style="visibility:hidden;">Hidden</label>
                    <div class="ui primary medium add button disabled">
                      <i class="plus icon"></i>
                      Add Details
                    </div>
                  </div>
                </div>
              </div>

              <div class="field">
                <label>Miscellaneous Items</label>
                <div class="fields">
                  <div class="seven wide field">
                    <%= label f, :misc_total_amount, "Total Amount" %>
                    <div class="ui right labeled input">
                      <%= text_input f, :misc_total_amount, disabled: true %>
                      <div class="ui label">
                        PHP
                      </div>
                    </div>
                  </div>
                  <div class="four wide field">
                    <label style="visibility:hidden;">Hidden</label>
                    <div class="ui primary medium add button disabled">
                      <i class="plus icon"></i>
                      Add Details
                    </div>
                  </div>
                </div>
              </div>

              <div class="field">
                <div class="fields">
                  <div class="seven wide field">
                    <%= label f, :or_and_dr_fee, "Operating and Delivery Room Fee" %>
                    <div class="ui right labeled input">
                      <%= text_input f, :or_and_dr_fee, disabled: true %>
                      <div class="ui label">
                        PHP
                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <div class="field <%#= check_mpf_state(@authorization.authorization_diagnosis) %>" id="MemberPayField">
                <div class="ui checkbox">
                  <input type="checkbox" class="<%#= check_mpf_state(@authorization.authorization_diagnosis) %>" name="authorization[isMemberPay][]" <%#= if is_nil(@authorization.special_approval_id), do: "checked", else: "" %> disabled>
                  <label class="coloring green"v><b>Member will pay for member excess.</b></label>
                </div>
              </div>

              <!--        <%# spec_state = if not is_nil(@authorization.special_approval_id), do: "", else: "hidden" %>
              <div class="field <%#= spec_state %>" id="sa_cont">
                <div class="ui grid">
                  <div class="twelve wide column">
                    <div class="field" id="sa_field">
                      <label>Special Approval</label>
                      <%#= select f, :special_approval_id, Enum.map(@special_approval, &{&1.text, &1.id}), prompt: "Special Approval", class: "ui selection dropdown" %>
                    </div>
                  </div>
                </div>
              </div> -->

              <div class="field">
                <label>Special Approval</label>
                <%= select f, :special_approval_id, Enum.map(@special_approval, &{&1.text, &1.id}), prompt: "Special Approval", class: "ui selection dropdown", disabled: true %>
              </div>

              <div class="field">
                <label>Internal Remarks</label>
                <%= if @authorization.status == "Disapproved" do %>
                  <%= textarea f, :internal_remarks, class: "form-control", readonly: true, value: "Reason for LOA Disapproval: #{@authorization.reason}" %>
                <%= else %>
                  <%= textarea f, :internal_remarks, class: "form-control", readonly: true %>
                <% end %>
                <%# <font color="red"><%= error_tag f, :internal_remarks %1></font> %>
              </div>
              <%= if @authorization.status == "Approved" do %>
                <a href="#" id="" class="ui default button mrg1T">
                  <i class="print icon"></i>
                  Print
                </a>
                <% can_resched = Date.compare(@authorization.valid_until, Ecto.Date.utc()) == :lt %>
                <%= if can_resched do %>
                  <div id="" class="ui blue left button mrg1T">
                    <i class="calendar icon"></i>
                    Reschedule LOA
                  </div>
                <% end %>
              <% end %>

              <%= if @authorization.status == "For Approval" do %>
                <div class="ui green  button mrg1T" id="btn_approve_loa" authorization_id="<%= @authorization.id %>">
                  Approve
                </div>
                <a class="ui default right floated button mrg1T" id="">
                  Edit
                </a>
                <div class="ui blue right floated buttons">
                  <div class="ui floating dropdown button" style="margin-top:14%">
                    Discharge
                    <div class="menu">
                      <div class="item"> Regular</div>
                      <div class="item"> Express</div>
                    </div>
                  </div>
                </div>
              <% end %>

              <%# <a href="#" id="btnSaveAuthorization" class="ui default left button mrg1T" authorization_id="<%= @authorization.id %1>"> %>
                <%#   <i class="save icon"></i> %>
                <%#   Save %>
                <%# </a> %>
              <%# <div id="submit" class="ui green left floated button mrg1T"> %>
                <%#   <i class="send icon"></i> %>
                <%#   Submit %>
                <%# </div> %>

              <%= if @authorization.status == "Draft" do %>
                <a href="#" id="" class="ui default left floated button mrg1T">
                  <i class="save icon"></i>
                  Save
                </a>
                <div id="" class="ui green left button mrg1T">
                  <i class="send icon"></i>
                  Submit
                </div>
                <a class="ui red right floated button mrg1T" id="btnDeleteDraft">
                  <i class="trash left icon"></i>
                  Delete Draft
                </a>

                <div class="ui blue right floated buttons">
                  <div class="ui floating dropdown button" style="margin-top:14%">
                    Discharge
                    <div class="menu">
                      <div class="item"> Regular</div>
                      <div class="item"> Express</div>
                    </div>
                  </div>
                </div>
              <% end %>

              <%= if @authorization.status == "Pending" do %>
                <a class="ui red right floated button mrg1T" id="btnCancelAuthorization" authorization_id="<%= @authorization.id %>">
                  <i class="close left icon"></i>
                  Cancel
                </a>
              <% end %>

              <%# <a href="#" id="btnSaveAuthorization" class="ui blue right floated button mrg1T" authorization_id="<%= @authorization.id %1>"> %>
                <%#   Discharge %>
                <%# </a> %>
            </div>
          </div>
        </div>
        <div class="five wide computer eleven wide tablet column">
          <div class="ui styled fluid accordion">
            <div class="title">
              <h3>
                Member
                <i class="dropdown icon" style="float: right"></i>
              </h3>
            </div>
            <div class="content">
              <div class="ui grid">
                <div class="eight wide computer  wide computer column">
                  <label>Name</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label><%= member_full_name(@member) %></label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label>Card Number</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label><%= @member.card_no %></label>
                </div>
                <%# <div class="eight wide computer eight wide computer column"> %>
                  <%#   <label>Member ID</label> %>
                  <%# </div> %>
                <%# <div class="eight wide computer eight wide computer column"> %>
                  <%#   <label><%= @member.id %1></label> %>
                  <%# </div> %>
                <div class="eight wide computer  wide computer column">
                  <label>Birth Date</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label id="birth_date"><%= format_birthdate(@member.birthdate) %></label>
                </div>
                <div class="eight wide computer eight wide computer column">
                  <label>Age</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label id="age">
                    <%= get_age(@member.birthdate) %>
                  </label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label>Gender</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label><%= @member.gender %></label>
                </div>
              </div>
            </div>
          </div>

          <div class="ui styled fluid accordion">
            <div class="title">
              <h3>
                Facility
                <i class="dropdown icon" style="float: right"></i>
              </h3>
            </div>
            <div class="content">
              <div class="ui grid">
                <div class="eight wide computer  wide computer column">
                  <label>Facility Code</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label><%= @authorization.facility.code %></label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label>Facility Name</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label><%= @authorization.facility.name %> </label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label>Facility Type</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label><%= @authorization.facility.type.text %> </label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label>Facility Category</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label><%= @authorization.facility.category.text %> </label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label>Address</label>
                </div>
                <div class="eight wide computer  wide computer column">
                  <label id="facility_address">
