<style>
  body {
    font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif;
    color: #333;
  }

  .panel {
    margin-bottom: 10px;
    background-color: #fff;
    border: 1px solid transparent;
    border-radius: 4px;
    -webkit-box-shadow: 0 1px 1px rgba(0,0,0,.05);
    box-shadow: 0 1px 1px rgba(0,0,0,.05);
  }
  .panel-default {
    border-color: gray;
  }
  .panel-default>.panel-heading {
    color: #FFF;
    background-color: #388e3c;
    border-color: gray;
    text-align: center;
  }
  .panel-heading {
    padding: 7px 15px;
    border-bottom: 1px solid transparent;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    font-size: 12px;
  }

  .panel-body {
    padding: 15px;
  }

  table {
    text-align: left;
    border-collapse: separate;
    border-spacing: 0;
    width: 100%;
    font-size: 12px;
  }

  /*th:first-child,
  td:first-child {
    padding: 0 15px 0 15px;
  }*/


  tbody tr:last-child td {
    border: none;
  }

  tbody td {
    line-height: 20px;
  }

  .ui.small.images .image,
  .ui.small.images img,
  .ui.small.images svg,
  .ui.small.image {
    width: 150px;
    height: auto;
    font-size: 0.92857143rem;
  }

  .ui.medium.images .image,
  .ui.medium.images img,
  .ui.medium.images svg,
  .ui.medium.image {
    width: 300px;
    height: auto;
    font-size: 1rem;
  }

  .no-border {
    border: none !important;
  }

  .pl {
    padding-left: 25px !important;
  }

  .mt_zero {
    margin-top: 0 !important;
  }

  .header {
    border-bottom: 1px solid rgba(34, 36, 38, 0.1) !important;
  }

  .text-white {
    color: #fff !important;
  }

  /*h5{
    font-size: 9px !important;
    margin: 10px 15px !important;
  }*/

  .header {
    border: none;
    margin: calc(2rem - 0.14285714em ) 0em 1rem;
    padding: 0em 0em;
    font-weight: bold;
    line-height: 1.28571429em;
    text-transform: none;
    color: rgba(0, 0, 0, 0.87);
  }

  .dividing.header {
    padding-bottom: 0.21428571rem;
    border-bottom: 1px solid rgba(34, 36, 38, 0.15);
  }

  .ui.basic.table {
    /*background: transparent;*/
    border: 1px solid rgba(34, 36, 38, 0.15);
    box-shadow: none;
  }

  .ui.basic.table thead,
  .ui.basic.table tfoot {
    box-shadow: none;
  }

  .ui.basic.table th {
    /*background: transparent;*/
    border-left: none;
  }

  .ui.basic.table tbody tr {
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  }

  .ui.basic.table td {
    background: transparent;
  }

  .ui.basic.striped.table tbody tr:nth-child(2n) {
    background-color: rgba(0, 0, 0, 0.05) !important;
  }

  .ui[class*="very basic"].table {
    border: none;
  }

  .ui[class*="very basic"].table:not(.sortable):not(.striped) th,
  .ui[class*="very basic"].table:not(.sortable):not(.striped) td {
    padding: '';
  }

  .ui[class*="very basic"].table:not(.sortable):not(.striped) th:first-child,
  .ui[class*="very basic"].table:not(.sortable):not(.striped) td:first-child {
    padding-left: 0em;
  }

  .ui[class*="very basic"].table:not(.sortable):not(.striped) th:last-child,
  .ui[class*="very basic"].table:not(.sortable):not(.striped) td:last-child {
    padding-right: 0em;
  }

  .ui[class*="very basic"].table:not(.sortable):not(.striped) thead tr:first-child th {
    padding-top: 0em;
  }

  .ui.table {
    width: 100%;
    /*background: #FFFFFF;*/
    margin: 1em 0em;
    /*border: 1px solid rgba(34, 36, 38, 0.15);*/
    box-shadow: none;
    /*border-radius: 0.28571429rem;*/
    text-align: left;
    color: rgba(0, 0, 0, 0.87);
    border-collapse: separate;
    border-spacing: 0px;
    padding: 0 15px 0 15px;
  }

  .ui.table:first-child {
    margin-top: 0em;
  }

  .ui.table:last-child {
    margin-bottom: 0em;
  }

  .ui.table th,
  .ui.table td {
    -webkit-transition: background 0.1s ease, color 0.1s ease;
    transition: background 0.1s ease, color 0.1s ease;
  }

  .ui.table thead {
    box-shadow: none;
  }

  .ui.table thead th {
    cursor: auto;
    /*background: #F9FAFB;*/
    text-align: inherit;
    color: rgba(0, 0, 0, 0.87);
    padding: 0.92857143em 0.78571429em;
    vertical-align: inherit;
    font-style: none;
    font-weight: bold;
    text-transform: none;
    border-bottom: 1px solid rgba(34, 36, 38, 0.1);
    border-left: none;
  }

  .ui.table thead tr > th:first-child {
    border-left: none;
  }

  .ui.table thead tr:first-child > th:first-child {
    border-radius: 0.28571429rem 0em 0em 0em;
  }

  .ui.table thead tr:first-child > th:last-child {
    border-radius: 0em 0.28571429rem 0em 0em;
  }

  .ui.table thead tr:first-child > th:only-child {
    border-radius: 0.28571429rem 0.28571429rem 0em 0em;
  }

  .ui.table tr td {
    padding-bottom: 0rem !important;
    padding-top: 0rem !important;
  }

  .ui.table tr:first-child td {
    border-top: none;
  }

  .ui.table td {
    padding: 0.78571429em 0.78571429em;
    text-align: inherit;
  }

  .hide {
    display: none;
  }
</style>
<body>
  <center>
    <%# <img class="ui medium image" src="https://preview.ibb.co/f51hRG/Medilink_Payor.png"> %>
    <img class="ui medium image" src="data:image/png;base64,">
  </center>

  <div class="ui divider"></div>

  <div class="panel panel-default">
    <div class="panel-heading">General</div>
    <div class="panel-body">
      <table id="table_general">
        <tr>
          <td style="width:50%">
            <table>
              <tr>
                <td><b>Account Code</b></td>
                <td><%= @account_group.code %></td>
              </tr>
              <tr>
                <td><b>Account Type</b></td>
                <td><%= @account_group.type %></td>
              </tr>
              <tr>
                <td><b>Industry</b></td>
                <td><%= if is_nil(@account_group.industry) == false, do: @account_group.industry.code %></td>
              </tr>
              <tr>
                <td><b>Effective Date</b></td>
                <td><%= @account.start_date %></td>
              </tr>
              <tr>
                <td><b>Telephone Number</b></td>
                <td><%= @account_group.phone_no %></td>
              </tr>
            </table>
          </td>
          <td>
            <table>
              <tr>
                <td><b>Account Name</b></td>
                <td><%= @account_group.name %></td>
              </tr>
              <tr>
                <td><b>Account Status</b></td>
                <td><%= @account.status %></td>
              </tr>
              <tr>
                <td><b>Segment</b></td>
                <td><%= @account_group.segment %></td>
              </tr>
              <tr>
                <td><b>Expiry Date</b></td>
                <td><%= @account.end_date %></td>
              </tr>
              <tr>
                <td><b>Website / Email Address</b></td>
                <td><%= @account_group.email %></td>
              </tr>
            </table>
          </td>
        </tr>
      </table>
    </div>
  </div>

  <div class="panel panel-default">
    <div class="panel-heading">Address</div>
    <div class="panel-body">
      <table>
        <tr>
          <td style="width:50%">
            <%= for address <- @account_group.account_group_address do %>
              <table id="table_address">
                <p><b><%= address.type %></b></p>
                <tr>
                  <td><b>Address Line 1</b></td>
                  <td><%= address.line_1 %></td>
                </tr>
                <tr>
                  <td><b>Address Line 2</b></td>
                  <td><%= address.line_2 %></td>
                </tr>
                <tr>
                  <td><b>City / Municipal</b></td>
                  <td><%= address.city %></td>
                </tr>
                <tr>
                  <td><b>Region</b></td>
                  <td><%= address.region %></td>
                </tr>
                <tr>
                  <td><b>Province</b></td>
                  <td><%= address.province %></td>
                </tr>
                <tr>
                  <td><b>Country</b></td>
                  <td><%= address.country %></td>
                </tr>
                <tr>
                  <td><b>Postal Code</b></td>
                  <td><%= address.postal_code %></td>
                </tr>
              </table>
            <% end %>
          </td>
          <td>
            <%= if Enum.count(@account_group.account_group_address) == 1 && Enum.at(@account_group.account_group_address, 0).is_check do %>
              <%= for address <- @account_group.account_group_address do %>
                <table id="table_billing_address">
                  <p><b>Billing Address</b></p>
                  <tr>
                    <td><b>Address Line 1</b></td>
                    <td><%= address.line_1 %></td>
                  </tr>
                  <tr>
                    <td><b>Address Line 2</b></td>
                    <td><%= address.line_2 %></td>
                  </tr>
                  <tr>
                    <td><b>City / Municipal</b></td>
                    <td><%= address.city %></td>
                  </tr>
                  <tr>
                    <td><b>Region</b></td>
                    <td><%= address.region %></td>
                  </tr>
                  <tr>
                    <td><b>Province</b></td>
                    <td><%= address.province %></td>
                  </tr>
                  <tr>
                    <td><b>Country</b></td>
                    <td><%= address.country %></td>
                  </tr>
                  <tr>
                    <td><b>Postal Code</b></td>
                    <td><%= address.postal_code %></td>
                  </tr>
                </table>
              <% end %>
            <% end %>
          </td>
        </tr>
      </table>
    </div>
  </div>

  <div class="panel panel-default">
    <div class="panel-heading">Contact</div>
    <div class="panel-body">
      <table id="table_contact" class="very basic table">
        <thead>
          <tr >
            <th>Contact Type</th>
            <th>Name</th>
            <th>Designation</th>
            <th>Telephone No</th>
            <th>Mobile No</th>
            <th>Fax No</th>
            <th>Email Address</th>
            <%# <th>CTC</th> %>
            <%# <th>Date Issued</th> %>
            <%# <th>Place Issued</th> %>
            <%# <th>Passport Number</th> %>
            <%# <th>Date Issued</th> %>
            <%# <th>Place Issued</th> %>
          </tr>
        </thead>
        <tbody>
          <%= if Enum.empty?(@account_group.account_group_contacts) do %>
            <tr>
              <td colspan="13">No Records Found!</td>
            </tr>
          <% else %>
            <%= for contacts <- @account_group.account_group_contacts do %>
              <tr>
                <td><%= contacts.contact.type %></td>
                <td><%= contacts.contact.first_name %> <%= contacts.contact.last_name %></td>
                <td><%= contacts.contact.designation %></td>
                <td>
                  <%= if Enum.empty?(contacts.contact.fax) do %>
                    No Record
                  <% else %>
                    <%= for telephone <- contacts.contact.phones do %>
                      <%= if telephone.type == "telephone" do %>
                        <%= "(" <> telephone.country_code <> ") " <> area_code(telephone.area_code) <> " " <> telephone.number <> " " <> local(telephone.local) %>
                      <% end %>
                    <% end %>
                  <% end %>
                </td>
                <td>
                  <%= for mobile <- contacts.contact.phones do %>
                    <%= if mobile.type == "mobile" do %>
                      <%= if is_nil(mobile.country_code) do %>
                        <%= mobile.number %>
                      <% else %>
                        <p><%= Enum.join(["(", mobile.country_code, ")", mobile.number]) %></p>
                      <% end %>
                    <% end %>
                  <% end %>
                </td>
                <td>
                  <%= if Enum.empty?(contacts.contact.fax) do %>
                    No Record
                  <% else %>
                    <%= for mobile <- contacts.contact.phones do %>
                      <%= if mobile.type == "fax" do %>
                        <%= "(" <> mobile.country_code <> ") " <> area_code(mobile.area_code) <> " " <> mobile.number <> " " <> local(mobile.local) %>
                      <% end %>
                    <% end %>
                  <% end %>
                </td>
                <td><%= contacts.contact.email %></td>
                <%# <td><%= contacts.contact.ctc %1></td> %>
                <%# <td><%= contacts.contact.ctc_date_issued %1></td> %>
                <%# <td><%= contacts.contact.ctc_place_issued %1></td> %>
                <%# <td><%= contacts.contact.passport_no %1></td> %>
                <%# <td><%= contacts.contact.passport_date_issued %1></td> %>
                <%# <td><%= contacts.contact.passport_place_issued %1></td> %>
              </tr>
            <% end %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>

  <div class="panel panel-default">
    <div class="panel-heading">Financial</div>
    <div class="panel-body" id="table_financial">
      <%= if is_nil(@account_group.payment_account) do %>
        <h4>No Records Found!</h4>
      <% else %>
        <table>
          <tr>
            <td valign="top" style="width:50%">
              <table>
                <tr>
                  <td><b>Funding Arrangement</b></td>
                  <td><%= @account_group.payment_account.funding_arrangement %></td>
                </tr>
                <tr>
                  <td><b>Tax Identification Number</b></td>
                  <td><%= @account_group.payment_account.account_tin %></td>
                </tr>
                <tr>
                  <td><b>Previous Carrier</b></td>
                  <td>
                    <%= if is_nil(@account_group.payment_account.previous_carrier) do %>
                      None
                    <% else %>
                      <%= @account_group.payment_account.previous_carrier %>
                    <% end %>
                  </td>
                </tr>
                <tr>
                  <td><b>Mode of Payment</b></td>
                  <td><%= @account_group.payment_account.mode_of_payment %></td>
                </tr>
                <tr>
                  <td><b>Bank Name</b></td>
                  <td>
                    <%= if is_nil(@account_group.bank) do %>
                      None
                    <% else %>
                      <%= @account_group.bank.account_name %>
                    <% end %>
                  </td>
                </tr>
              </table>
            </td>
            <td valign="top">
              <table>
                <tr>
                  <td><b>VAT Status</b></td>
                  <td><%= @account_group.payment_account.vat_status %></td>
                </tr>
                <tr>
                  <td><b>Attached Point</b></td>
                  <td>
                    <%= if is_nil(@account_group.payment_account.attached_point) do %>
                      None
                    <% else %>
                      <%= @account_group.payment_account.attached_point %>
                    <% end %>
                  </td>
                </tr>
                <tr>
                  <td><b>Bank Account Number</b></td>
                  <td>
                    <%= if is_nil(@account_group.bank) do %>
                      None
                    <% else %>
                      <%= @account_group.bank.account_no %>
                    <% end %>
                  </td>
                </tr>
                <tr>
                  <td><b>Branch</b></td>
                  <td>None</td>
                </tr>
              </table>
            </td>
          </tr>
        </table>
        <%= if Enum.count(@account_group.account_group_coverage_funds) > 0 do %>
          <br/>
          <table>
            <p style="font-size:12px"><b>Coverage Funds</b></p>
            <thead>
              <tr>
                <th>Coverage</th>
                <th>Revolving Fund</th>
                <th>Replenish Threshold</th>
              </tr>
            </thead>
            <tbody>
              <%= for coverage_fund <- @account_group.account_group_coverage_funds do %>
                <tr>
                  <td><%= coverage_fund.coverage.name %></td>
                  <td><%= coverage_fund.revolving_fund %></td>
                  <td><%= coverage_fund.replenish_threshold %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        <% end %>
      <% end %>
    </div>
  </div>

  <div class="panel panel-default">
    <div class="panel-heading">Hierarchy of Eligible Dependents</div>
    <div class="panel-body" id="table_hoed">
      <table>
        <tr>
          <td valign="top">
            <table>
              <tr><th><b>Married Employe</b></th></tr>
              <%= for result <- load_ahoed_summary(@account_group.account_hierarchy_of_eligible_dependents, "Married Employee") do %>
                <tr><td><%= result %></td></tr>
              <% end %>
            </table>
          </td>
          <td valign="top">
            <table>
              <tr><th><b>Single Employe</b></th></tr>
              <%= for result <- load_ahoed_summary(@account_group.account_hierarchy_of_eligible_dependents, "Single Employee") do %>
                <tr><td><%= result %></td></tr>
              <% end %>
            </table>
          </td>
          <td valign="top">
            <table>
              <tr><th><b>Single Parent Employe</b></th></tr>
              <%= for result <- load_ahoed_summary(@account_group.account_hierarchy_of_eligible_dependents, "Single Parent Employee") do %>
                <tr><td><%= result %></td></tr>
              <% end %>
            </table>
          </td>
        </tr>
      </table>
    </div>
  </div>
</body>
