<div class="main-wrapper">
  <div class="breadcrumbs">
    <ul>
      <li><%= gettext "Home" %></li><li><i class="chevron right icon"></i></li>
      <li><a href="/<%= @locale %>/members"><%= gettext "Member" %></a></li><li><i class="chevron right icon"></i></li>
      <li class="active"><%= @member.first_name %> <%= @member.last_name %></li>
    </ul>
  </div>

  <div class="sections-wrapper">
    <div class="ui container">
      <div class="ui grid">
        <div class="row">
          <div class="sixteen wide computer column">
            <section>
              <div class="ui grid">
                <div class="row">
                  <div class="three wide computer four wide tablet sixteen wide mobile column">
                    <div class="profile__account-logo">
                      <%= if is_nil(@member.photo) do %>
                        <img id="photo" src="/images/no-photo-available.png">
                      <% else %>
                        <img id="photo" src="<%= get_img_url(@member) %>">
                      <% end %>
                    </div>
                  </div>

                  <div class="thirteen wide computer twelve wide tablet sixteen wide mobile column mobile-mt-2">
                    <div class="profile__header">
                      <h1 class="d-inline-block ui center aligned header">
                        <%= member_full_name(@member) %>
                        <%= if @member.vip do %>
                          <div class="ui yellow label">
                            <span>VIP</span>
                          </div>
                        <% end %>
                      </h1>
                      <div class="float-right">
                        <div class="ui icon top right floated pointing dropdown basic button">
                          <i class="list icon"></i>
                          <div class="left menu">
                          <%= if Enum.member?(["Active", "Pending", "For Approval"], @member.status) do %>
                            <div class="item clickable-row" href="/<%= @locale %>/members/<%= @member.id %>/edit?step=1">
                              <i class="edit icon"></i>
                              Edit
                            </div>
                          <% end %>
                        </div>
                      </div>
                    </div>
                      <div class="f-blue bold uppercase"><%= @member.id %>, <%= @member.type %> / <%= @member.account_group.code %>, <%= @member.account_group.segment %></div>
                      <div class="mt-2">
                        <div class="ui two column grid">
                          <div class="column">
                            <div class="ui two column grid">
                              <div class="row">
                                <div class="column">
                                  <b><%= gettext "Birthdate" %></b>
                                </div>
                                <div class="column">
                                  <%= @member.birthdate  %>
                                </div>
                              </div>
                              <div class="row">
                                <div class="column">
                                  <b><%= gettext "Effective Date" %></b>
                                </div>
                                <div class="column">
                                  <%= @member.effectivity_date %>
                                </div>
                              </div>
                              <div class="row">
                                <div class="column">
                                  <b><%= gettext "Expiry Date" %></b>
                                </div>
                                <div class="column">
                                  <%= @member.expiry_date %>
                                </div>
                              </div>
                              <div class="row">
                                <div class="column">
                                  <b><%= gettext "Funding Arrangement" %></b>
                                </div>
                                <div class="column">
                                  <%= @member.account_group.payment_account.funding_arrangement %>
                                </div>
                              </div>
                              <div class="row">
                                <div class="column">
                                  <b><%= gettext "Status" %></b>
                                </div>
                                <div class="column">
                                  <%= render "member_status.html", member: @member %>
                                </div>
                              </div>
                            </div>
                          </div>
                          <div class="column">
                            <div class="ui two column grid">
                              <%= if not is_nil(@member.suspend_date) and @member.status == "Active" do %>
                                <div class="row">
                                  <div class="column">
                                    <b><%= gettext "For Suspension" %></b>
                                  </div>
                                  <div class="column">
                                    <%= @member.suspend_date %>
                                  </div>
                                </div>
                              <% end %>
                              <%= if not is_nil(@member.cancel_date) and @member.status != "Cancelled" do %>
                                <div class="row">
                                  <div class="column">
                                    <b><%= gettext "For Cancellation" %></b>
                                  </div>
                                  <div class="column">
                                    <%= @member.cancel_date %>
                                  </div>
                                </div>
                              <% end %>
                              <%= if not is_nil(@member.reactivate_date) do %>
                                <div class="row">
                                  <div class="column">
                                    <b><%= gettext "For Reactivation" %></b>
                                  </div>
                                  <div class="column">
                                    <%= @member.reactivate_date %>
                                  </div>
                                </div>
                              <% end %>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
            </section>

            <section>
