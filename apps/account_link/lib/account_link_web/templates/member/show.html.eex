<div class="ui large modal" id="modalProcedure">
  <i class="close icon"></i>
  <div class="header">
    <%= gettext "Add Product" %>
  </div>
  <div class="content">
    <%= form_for @conn, member_path(@conn, :add_member_product, @locale, @member.id), [as: :product], fn _f -> %>
      <input type="hidden" name="_csrf_token" value="<%= Plug.CSRFProtection.get_csrf_token() %>">
      <input type="hidden" value="" name="member[account_product_ids_main]">
      <table role="datatable" class="ui stripped table">
        <thead>
          <tr>
            <th>
              <div class="ui checkbox">
                <input type="checkbox" id="product_select">
              </div>
            </th>
            <th><%= gettext "Code" %></th>
            <th><%= gettext "Name" %></th>
            <th><%= gettext "No. of Benefits" %></th>
            <th><%= gettext "Limit Type" %></th>
            <th><%= gettext "Limit Amount" %></th>
          </tr>
        </thead>
        <tbody>
          <%= for product <- filter_account_group_products(@member) do %>
            <tr class='<%= if check_product_age_eligibility(@member, product) == false, do: "disabled" %>'>
              <td>
                <div class="ui checkbox">
                  <input type="checkbox" name="member[account_product_id][]" value="<%= product.id %>" />
                </div>
              </td>
              <%= if check_product_age_eligibility(@member, product) do %>
                <td><%= product.code %></td>
                <% else %>
                <td><%= product.code %> (Not eligible for memberâ€™s age)</td>
              <% end %>
              <td><%= product.name %></td>
              <td><%= product.number_of_benefits %></td>
              <td><%= product.limit_type %></td>
              <td><%= product.limit_amount %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
  </div>
  <div class="actions">
    <a class="ui basic deny button">
      <i class="remove icon"></i>
      <%= gettext "Cancel" %>
    </a>
    <button class="ui button" type="submit">
      <i class="plus icon"></i>
      <%= gettext "Add" %>
    </button>
  </div>
<% end %>
</div>
