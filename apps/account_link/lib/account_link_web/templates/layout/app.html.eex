<!DOCTYPE html>
<html lang="en">
  <head>
    <style>
      html {
        display: none;
      }
    </style>
    <script>
      if (self == top) {
        document.documentElement.style.display = 'block';
      } else {
        top.location = self.location;
      }
    </script>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Medilink - Home</title>
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="<%= static_path(@conn, "/css/app.css") %>">
  </head>
  <body>
    <header class="header">
      <div class="ui menu">
        <div class="ui container">
          <div class="item">
            <a href="#" class="mr-3 fs-20 hamburger-trigger"><i class="sidebar icon"></i></a>

            <a href="/">
              <img class="site-logo" src="/images/logo.png">
            </a><span class="acc-gateway"><%= gettext "accountgateway" %></span>
          </div>
          <div class="right item">
            <span class="date-time" id="date-time">
              <span class="dark" id="day"></span>
              <span class="light" id="month-year"></span>
              <span class="light">|</span>
              <span class="dark" id="time"></span>
              <span class="light" id="indicator"></span>
            </span>
            <a><i class="alarm outline icon"></i></a>
            <%# <a><i class="user outline icon"></i>Admin<i class="angle down icon"></i></a> %>
            <div class="ui simple dropdown">
              <i class="world icon"></i>
              <span class="text">Select Language</span>
              <div class="menu">
                <div class="item">Chinese</div>
                <div class="item">English</div>
              </div>
              <i class="angle down icon"></i>
            </div>

            <div class="ui simple dropdown">
              <%= if @current_user do %>
                <i class="user outline icon"></i>
                <a href="#"> <%= @current_user.username %></a>
                <i class="angle down icon"></i>
                <div class="menu">
                  <%= link "Change Password", to: user_path(@conn, :change_password, @locale), method: :get, class: "item" %>
                  <%= link "Logout", to: session_path(@conn, :logout, @locale), method: :delete, data: [confirm: "Are you sure?"], class: "item" %>
                </div>
              <% else %>
                <%= link "Sign in", to: session_path(@conn, :login, @locale) %>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </header>

    <div class="ui left vertical visible sidebar labeled icon menu">
