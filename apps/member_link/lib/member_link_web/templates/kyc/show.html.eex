<div class="pusher">
  <div class="ui container">
    <div class="ui grid">
      <div class="sixteen wide column"  style="margin-top: 3.5rem;">
        <h1 class="ui huge header">Bank KYC Show</h1>
        <div class="ui breadcrumb">
        </div>
        <div class="ui divider"></div>
        <div class="ui stackable grid">


<div class="row">
  <div class="sixteen wide column">
      <h3> Personal Information </h3>
      <div class="ui divider"></div>
      <div class="ui two fields grid container">
        <div class="two wide field two wide column">
          <label><b>Place of Birth</b></label>
        </div>
        <div class="six wide field two wide column">
          <label><%= @kyc.city <> ", " <> @kyc.country %></label>
        </div>

        <div class="three wide field two wide column">
          <label><b>Citizenship</b></label>
        </div>
        <div class="five wide field two wide column">
          <label><%= @kyc.citizenship %></label>
        </div>

        <div class="two wide field two wide column">
          <label><b>Civil Status</b></label>
        </div>
        <div class="six wide field two wide column">
          <label><%= @kyc.civil_status %></label>
        </div>

        <div class="three wide field two wide column">
          <label><b>Mother's Maiden Name</b></label>
        </div>
        <div class="five wide field two wide column">
          <label><%= @kyc.mm_last_name <> ", " <> @kyc.mm_first_name <> " " <> @kyc.mm_middle_name %></label>
        </div>


      </div>
      <br><br>
      <h3> Educational and Work Information </h3>
      <div class="ui divider"></div>
      <div class="ui two fields grid container">
        <div class="three wide field two wide column">
          <label><b>Educational Attainment</b></label>
        </div>
        <div class="five wide field two wide column">
          <label><%= @kyc.educational_attainment %></label>
        </div>

        <div class="three wide field two wide column">
          <label><b>Company</b></label>
        </div>
        <div class="five wide field two wide column">
          <label><%= @kyc.company_name %></label>
        </div>

        <div class="three wide field two wide column">
          <label><b>Company Branch</b></label>
        </div>
        <div class="five wide field two wide column">
          <label><%= @kyc.company_branch %></label>
        </div>


        <div class="three wide field two wide column">
          <label><b>Position Title</b></label>
        </div>
        <div class="five wide field two wide column">
          <label><%= @kyc.position_title %></label>
        </div>

        <div class="three wide field two wide column">
          <label><b>Occupation</b></label>
        </div>
        <div class="five wide field two wide column">
          <label><%= @kyc.occupation %></label>
        </div>

        <div class="three wide field two wide column">
          <label><b>Nature of Work</b></label>
        </div>
        <div class="five wide field two wide column">
          <label><%= @kyc.nature_of_work %></label>
        </div>

        <div class="three wide field two wide column">
          <label><b>Source of Fund</b></label>
        </div>
        <div class="five wide field two wide column">
          <label><%= @kyc.source_of_fund %></label>
        </div>
      </div>

      <br><br>
      <h3> Contact Information </h3>
      <div class="ui divider"></div>
      <div class="ui two fields grid container">
        <div class="three wide field two wide column">
          <label><b>Address</b></label>
        </div>
        <div class="thirteen wide field two wide column">
          <label><%= @kyc.street_no <> ", " <> @kyc.subd_dist_town <> ", " <> @kyc.city <> ", " <> @kyc.country %></label>
        </div>

        <div class="three wide field two wide column">
          <label><b>Mobile Number</b></label>
        </div>
        <div class="five wide field two wide column">
          <%= if not is_nil(@kyc.mobile1) do %>
            <label><%= @kyc.mobile1 %></label>
          <% else %>
            <label>N/A</label>
          <% end %>
        </div>

        <div class="three wide field two wide column">
          <label><b>Secondary Mobile Number</b></label>
        </div>
        <div class="five wide field two wide column">
          <%= if not is_nil(@kyc.mobile2) do %>
            <label><%= @kyc.mobile2 %></label>
          <% else %>
            <label>N/A</label>
          <% end %>
        </div>

        <div class="three wide field two wide column">
          <label><b>Email Address</b></label>
        </div>
        <div class="five wide field two wide column">
          <%= if not is_nil(@kyc.email1) do %>
            <label><%= @kyc.email1 %></label>
          <% else %>
            <label>N/A</label>
          <% end %>
        </div>

        <div class="three wide field two wide column">
          <label><b>Secondary Email Address</b></label>
        </div>
        <div class="five wide field two wide column">
          <%= if not is_nil(@kyc.email2) do %>
            <label><%= @kyc.email2 %></label>
          <% else %>
            <label>N/A</label>
          <% end %>
        </div>
      </div>

      <%# <h3> Emergency Contact Information </h3> %>
