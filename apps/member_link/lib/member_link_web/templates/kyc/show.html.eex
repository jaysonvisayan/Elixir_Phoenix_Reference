        <%# </div> %>

        <%# <div class="three wide field two wide column"> %>
          <%# <label><b>Allergies</b></label> %>
        <%# </div> %>
        <%# <div class="five wide field two wide column"> %>
          <%# <label><%= #@kyc.allergies %>
            <%# </label> %>
        <%# </div> %>

        <%# <div class="three wide field two wide column"> %>
          <%# <label><b>Medication</b></label> %>
        <%# </div> %>
        <%# <div class="five wide field two wide column"> %>
          <%# <label><%= #@kyc.medication %>
          <%# </label> %>
        <%# </div> %>


        <%# <div class="three wide field two wide column"> %>
          <%# <label><b>Emergency Contact Person</b></label> %>
        <%# </div> %>
        <%# <div class="five wide field two wide column"> %>
          <%# <label>Junnie Mindanaon</label> %>
        <%# </div> %>

        <%# <div class="three wide field two wide column"> %>
          <%# <label><b>Phone Number</b></label> %>
        <%# </div> %>
